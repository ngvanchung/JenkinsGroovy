node {
    stage('SCM') {
    	echo 'Gathering code from version control'
	git branch: '${branch}', url: 'https://github.com/FeynmanFan/JenkinsGroovy.git'
    }
    stage('Build') {
    	try {
        echo 'Building....'
	powershell 'dotnet --version'
	powershell 'dotnet build ConsoleApp1'
	echo 'Build DONE'
	//releasenotes()
	echo 'Building new feature (ConsoleApp1)'
	} catch(ex) {
		echo 'Something went wrong'
		echo ex.toString();
		currentBuild.result = 'FAILURE'
	}
	finally {
		// cleanup
	}
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}
